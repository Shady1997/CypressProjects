{
  "name": "@benmalka/foxdriver",
  "version": "0.4.1",
  "author": "Ben Malka <ben2000567@gmail.com>",
  "description": "Foxdriver is a Node library which provides a high-level API to control Firefox over the Remote Debugging Protocol",
  "license": "Apache-2.0",
  "homepage": "https://github.com/benmalka/foxdriver#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benmalka/foxdriver.git"
  },
  "keywords": [
    "firefox",
    "remote",
    "debugging",
    "debug",
    "protocol",
    "cdp",
    "devtools"
  ],
  "bugs": {
    "url": "https://github.com/benmalka/foxdriver/issues"
  },
  "main": "build/index.js",
  "scripts": {
    "build": "run-s clean compile",
    "ci": "run-s eslint test:unit:cover",
    "clean": "rm -rf ./build",
    "compile": "babel lib/ -d build/ --copy-files --ignore config --source-maps inline",
    "eslint": "eslint ./lib/**/*.js",
    "release": "np patch",
    "release:patch": "np patch",
    "release:minor": "np minor",
    "release:major": "np major",
    "test": "run-p test:*",
    "test:lint": "eslint ./lib ./test",
    "test:unit": "jest",
    "test:unit:cover": "jest --coverage",
    "watch": "npm run compile -- --watch"
  },
  "dependencies": {
    "fs-extra": "^4.0.1",
    "get-port": "^3.2.0",
    "npmlog": "^4.1.2",
    "safe-buffer": "^5.1.1",
    "tcp-port-used": "^1.0.1",
    "tmp": "0.0.33"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/runtime": "^7.4.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "firefox-profile": "^1.2.0",
    "geckodriver": "^1.14.1",
    "jest": "^24.1.0",
    "np": "^2.16.0",
    "npm-run-all": "^4.0.2",
    "sinon": "^3.2.0",
    "webdriver": "^5.7.15"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 72,
        "functions": 93,
        "lines": 89,
        "statements": 89
      }
    }
  }
}
